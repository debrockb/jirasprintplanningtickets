{
  "name": "jira-card-printer-forge",
  "version": "1.0.0",
  "description": "Jira Cloud Card Printer - Design and print custom ticket/card layouts from Jira issues with AI-powered field processing",
  "main": "src/index.ts",
  "scripts": {
    "build": "forge deploy",
    "deploy:dev": "forge deploy --environment development",
    "deploy:staging": "forge deploy --environment staging",
    "deploy:prod": "forge deploy --environment production",
    "tunnel": "forge tunnel",
    "logs": "forge logs --tail"
  },
  "dependencies": {
    "@forge/api": "^3.0.0",
    "@forge/resolver": "^1.5.0",
    "@forge/bridge": "^3.0.0"
  },
  "devDependencies": {
    "@forge/cli": "^6.0.0",
    "typescript": "^5.5.2",
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "jira",
    "atlassian",
    "forge",
    "cards",
    "printing",
    "ai",
    "ticket-printer"
  ],
  "author": "",
  "license": "MIT"
}
